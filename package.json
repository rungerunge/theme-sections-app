{
  "name": "okayscale-sections",
  "version": "1.0.0",
  "description": "Shopify app for managing custom theme sections",
  "private": true,
  "main": "simple-server.js",
  "license": "UNLICENSED",
  "scripts": {
    "test": "jest --passWithNoTests .",
    "start": "node simple-server.js",
    "dev": "cd web && npm run dev",
    "build": "node build.js",
    "shopify": "cd web && npm run shopify",
    "version-check": "node version-check.js",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "codegen": "shopify app codegen",
    "debug": "node --inspect-brk build.js"
  },
  "keywords": [
    "shopify",
    "sections",
    "themes"
  ],
  "author": "OkayScale",
  "dependencies": {
    "@remix-run/node": "1.19.3",
    "@remix-run/react": "1.18.1",
    "@remix-run/serve": "1.18.1",
    "@shopify/app": "3.47.0",
    "@shopify/app-bridge": "3.7.8",
    "@shopify/app-bridge-react": "3.7.8",
    "@shopify/cli": "3.47.0",
    "@shopify/polaris": "11.1.2",
    "@shopify/shopify-api": "7.7.0",
    "@shopify/shopify-app-remix": "1.3.0",
    "@shopify/shopify-app-session-storage": "1.1.0",
    "express": "4.18.2",
    "axios": "1.6.2",
    "cors": "2.8.5",
    "express-fileupload": "1.4.0",
    "cross-env": "7.0.3",
    "dotenv": "16.3.1",
    "isbot": "3.6.8",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "1.18.1",
    "@remix-run/eslint-config": "1.18.1",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.8.0",
    "prettier": "2.8.8"
  },
  "workspaces": [
    "web",
    "extensions/*"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}
